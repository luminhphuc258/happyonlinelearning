<main>
  <!-- Start main body -->
  {{!-- breadcrumb --}}
  <ul class="flex items-center justify-left font-[sans-serif] space-x-4 mt-4 ml-5">
    <li class="text-gray-500 text-base cursor-pointer">
      <a href="/getassignmentpage">Your Courses</a>
    </li>
    <li>
      <svg xmlns="http://www.w3.org/2000/svg" class="fill-gray-400 w-3.5 -rotate-90" viewBox="0 0 24 24">
        <path fill-rule="evenodd"
          d="M11.99997 18.1669a2.38 2.38 0 0 1-1.68266-.69733l-9.52-9.52a2.38 2.38 0 1 1 3.36532-3.36532l7.83734 7.83734 7.83734-7.83734a2.38 2.38 0 1 1 3.36532 3.36532l-9.52 9.52a2.38 2.38 0 0 1-1.68266.69734z"
          clip-rule="evenodd" data-original="#000000"></path>
      </svg>
    </li>
    <li class="text-gray-800 text-base font-bold cursor-pointer">
      <a href="/assignmentslist">List of Assignments</a>
    </li>
  </ul>

  {{!-- end --}}
  <h4 class="text-red-800 text-2xl  font-bold mb-5 text-center mt-5 bg-blue">
    &nbsp;A List of "{{courname}}" assignments</b>
  </h4>

  {{!-- end filter date controll --}}
  {{!-- program lists --}}
  {{#each returnlists}}
  <div class="font-[sans-serif] mt-4">
    <div class="space-y-4 max-w-5xl mx-auto mt-4">
      {{!-- sub items --}}
      <div class="shadow-[0_2px_10px_-3px_rgba(6,81,237,0.3)] rounded-lg border-l-8 border-blue-600" role="accordion">
        <button type="button" onclick="setAssignmentID({{this.assignment_id}});"
          class="accordion-button w-full text-sm font-semibold text-left py-5 px-6 text-blue-800 flex items-center">
          <svg xmlns="http://www.w3.org/2000/svg" class="fill-current w-8 mr-4 shrink-0" viewBox="0 0 512 512">
            <path
              d="M227.656 357.508a15.13 15.13 0 0 0 10.692 4.426c4.007 0 7.851-1.59 10.687-4.426l53.973-53.973c5.906-5.906 5.906-15.476 0-21.383-5.903-5.902-15.477-5.902-21.38 0l-43.28 43.282-19.735-19.735c-5.906-5.902-15.476-5.902-21.383 0-5.902 5.903-5.902 15.477 0 21.383zm0 0"
