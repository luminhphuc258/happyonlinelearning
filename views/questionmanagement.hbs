<!-- Start main body -->
<main>
  {{!-- breadcrumb --}}
  <ul class="flex items-center justify-left font-[sans-serif] space-x-4 mt-4 ml-5">
    <li class="text-gray-500 text-base cursor-pointer">
      <a href="/fetchOurAllQuizzes">Quizzes</a>
    </li>
    <li>
      <svg xmlns="http://www.w3.org/2000/svg" class="fill-gray-400 w-3.5 -rotate-90" viewBox="0 0 24 24">
        <path fill-rule="evenodd"
          d="M11.99997 18.1669a2.38 2.38 0 0 1-1.68266-.69733l-9.52-9.52a2.38 2.38 0 1 1 3.36532-3.36532l7.83734 7.83734 7.83734-7.83734a2.38 2.38 0 1 1 3.36532 3.36532l-9.52 9.52a2.38 2.38 0 0 1-1.68266.69734z"
          clip-rule="evenodd" data-original="#000000"></path>
      </svg>
    </li>
    <li class="text-gray-800 text-base font-bold cursor-pointer">
      <a href="/getallquestions">Questions</a>
    </li>
  </ul>

  {{!-- end --}}
  {{!-- search area --}}
  <br>
  <div
    class="bg-white flex px-1 py-1 rounded-full border border-green-500 overflow-hidden max-w-md mx-auto font-[sans-serif]">
    <input type='text' id="textinputsearch" placeholder='Search Something...'
      class="w-full outline-none bg-white pl-4 text-sm" />
    <button type='button' id="btnSearch"
      class="bg-green-600 hover:bg-green-700 transition-all text-white text-sm rounded-full px-5 py-2.5">Search</button>
  </div>
  <br>
  {{!-- end search --}}
  {{!-- button --}}
  <hr>
  <button onclick="AddNewQuestions();" type="button"
    class="mb-5 mt-5 ml-5 px-5 py-2.5 rounded-full text-white text-sm tracking-wider font-medium border border-current outline-none bg-green-700 hover:bg-green-800 active:bg-green-700">Add
    Question for <span id="titlequize"></span></button>

  {{!-- end button --}}
  <div class="font-[sans-serif] overflow-x-auto">
    <table id="userliststable" class="min-w-full bg-white border border-green-300 cursor-pointer">
      <thead class="whitespace-nowrap bg-green-300">
        <tr>
          <th class="p-4 text-left text-sm font-semibold text-black">Question Content</th>
          <th class="p-4 text-left text-sm font-semibold text-black">Answer</th>
          <th class="p-4 text-left text-sm font-semibold text-black">Total Point</th>
          <th class="p-4 text-left text-sm font-semibold text-black">Action</th>
        </tr>
      </thead>
      <tbody class="whitespace-nowrap">
        {{#each questions}}
        <tr class="odd:bg-green-50 hover:bg-green-200">
          <!-- Level 1: Quiz Title, Publish, and Action -->
          <td class="p-4 text-sm">
            <div class="flex items-center cursor-pointer w-max">
              <div class="ml-4">
                <p class="text-sm text-black">{{this.content}}</p>
              </div>
            </div>
          </td>
          <td class="p-4 text-sm">
            <div class="flex items-center cursor-pointer w-max">
              <div class="ml-4">
                <p class="text-sm text-black">{{this.answer}}</p>
              </div>
            </div>
          </td>
          <td class="p-4 text-sm">
            <div class="flex items-center cursor-pointer w-max">
              <div class="ml-4">
                <p class="text-sm text-black">{{this.score}}</p>
              </div>
            </div>
          </td>
          <td class="p-4">
            {{!-- update --}}
            <!-- Action Buttons for Edit and Delete -->
            <button title="Delete" onclick="show_Modal_delete('{{this.id}}');">
              <svg xmlns="http://www.w3.org/2000/svg" class="w-5 fill-red-500 hover:fill-red-700" viewBox="0 0 24 24">
                <path
                  d="M19 7a1 1 0 0 0-1 1v11.191A1.92 1.92 0 0 1 15.99 21H8.01A1.92 1.92 0 0 1 6 19.191V8a1 1 0 0 0-2 0v11.191A3.918 3.918 0 0 0 8.01 23h7.98A3.918 3.918 0 0 0 20 19.191V8a1 1 0 0 0-1-1Zm1-3h-4V2a1 1 0 0 0-1-1H9a1 1 0 0 0-1 1v2H4a1 1 0 0 0 0 2h16a1 1 0 0 0 0-2ZM10 4V3h4v1Z" />
                <path d="M11 17v-7a1 1 0 0 0-2 0v7a1 1 0 0 0 2 0Zm4 0v-7a1 1 0 0 0-2 0v7a1 1 0 0 0 2 0Z" />
              </svg>
            </button>
          </td>
        </tr>
        <!-- Level 2: Nested Table for Additional Details -->
        <tr class="bg-green-100">
          <td colspan="4">
            <table class="min-w-full bg-white border border-gray-200">
              <thead>
                <tr>
                  <th class="p-4 text-left text-sm font-semibold text-black">option 1</th>
                  <th class="p-4 text-left text-sm font-semibold text-black">option 2</th>
                  <th class="p-4 text-left text-sm font-semibold text-black">option 3</th>
                  <th class="p-4 text-left text-sm font-semibold text-black">option 4</th>
                </tr>
              </thead>
              <tbody>
                <tr>
                  <td class="p-4 text-sm">
                    <div class="ml-4">
                      <p class="text-sm text-black">{{this.option1}}</p>
                    </div>
                  </td>
                  <td class="p-4 text-sm">
                    <div class="ml-4">
                      <p class="text-sm text-black">{{this.option2}}</p>
                    </div>
                  </td>
                  <td class="p-4 text-sm">
                    <div class="ml-4">
                      <p class="text-sm text-black">{{this.option3}}</p>
                    </div>
                  </td>
                  <td class="p-4 text-sm">
                    <div class="ml-4">
                      <p class="text-sm text-black">{{this.option4}}</p>
                    </div>
                  </td>
                </tr>
              </tbody>
            </table>
          </td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
  <!-- End Body -->
</main>

